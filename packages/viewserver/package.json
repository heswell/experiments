{
  "name": "@heswell/viewserver",
  "version": "1.5.2",
  "description": "Heswell Viewserver",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist/*",
    "build": "rollup -c --environment BUILD:production",
    "build:dev": "rollup -c --environment BUILD:development",
    "publish:local": "npm run build && npm version patch && npm publish",
    "publish:npm": "npm publish --registry=https://registry.npmjs.org"
  },
  "author": "heswell",
  "license": "MIT",
  "dependencies": {
    "@heswell/data-store": "^1.2.2",
    "@heswell/utils": "^1.3.4"
  },
  "devDependencies": {
    "rollup": "1.29.0",
    "rollup-plugin-copy": "^3.1.0",
    "rollup-plugin-node-resolve": "^5.2.0"
  },
  "gitHead": "fe0931a17f09cd63557bf9fd4bc71d6c775ed13d"
}
