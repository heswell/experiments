{
  "name": "@heswell/viewserver",
  "version": "2.0.11",
  "description": "Heswell Viewserver",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist/*",
    "build": "rollup -c --environment BUILD:production",
    "build:dev": "rollup -c --environment BUILD:development",
    "publish:local": "npm run build && npm version patch && npm publish",
    "publish:npm": "npm publish --registry=https://registry.npmjs.org"
  },
  "author": "heswell",
  "license": "MIT",
  "dependencies": {
    "@heswell/data-store": "^2.0.4",
    "@heswell/utils": "^2.0.12"
  },
  "devDependencies": {
    "rollup": "1.29.0",
    "rollup-plugin-copy": "^3.1.0",
    "rollup-plugin-node-resolve": "^5.2.0"
  },
  "gitHead": "4607a2bf8ad64e691e619304ad931b52cb513eb8"
}
